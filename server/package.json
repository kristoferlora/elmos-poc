{
  "name": "electric-monitoring-system-server",
  "version": "1.0.0",
  "description": "System that monitors electricty usage",
  "main": "src/app.js",
  "scripts": {
    "dev": "nodemon ./src/app.js --exec babel-node",
    "docs": "./node_modules/.bin/jsdoc -r src -d docs -c ./.jsdoc -t ./node_modules/docdash",
    "build": "babel src -d dist -s --presets es2015",
    "serve": "node dist/app.js",
    "start": "npm run build && npm run serve"
  },
  "author": "Kristofer Lora",
  "license": "ISC",
  "dependencies": {
    "bcrypt": "1.0.3",
    "body-parser": "1.18.2",
    "cors": "2.8.4",
    "crypto-js": "3.1.9-1",
    "debug": "3.1.0",
    "dotenv": "4.0.0",
    "express": "^4.16.4",
    "express-graphql": "0.7.1",
    "express-interceptor": "1.2.0",
    "graphql": "0.12.3",
    "graphql-custom-types": "1.5.0",
    "graphql-errors": "2.1.0",
    "graphql-fields": "1.1.0",
    "graphql-iso-date": "3.6.1",
    "graphql-request": "1.4.1",
    "graphql-tools": "2.18.0",
    "helmet": "^3.15.0",
    "joi": "13.1.1",
    "jsonwebtoken": "8.0.0",
    "lodash": "^4.17.11",
    "moment": "2.19.3",
    "moment-timezone": "0.5.21",
    "path": "0.12.7",
    "pg": "6.1.0",
    "redis": "2.8.0",
    "request": "2.88.0",
    "request-promise": "4.2.2",
    "sequelize": "4.8.0",
    "url-parse": "^1.4.4",
    "uuid": "3.3.2"
  },
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-core": "6.26.3",
    "babel-eslint": "7.2.3",
    "babel-plugin-transform-object-rest-spread": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "docdash": "1.0.0",
    "eslint": "^5.5.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-react": "^7.11.0",
    "expect": "1.20.2",
    "faker": "^4.1.0",
    "jsdoc": "3.5.5",
    "mocha": "^5.2.0",
    "nodemon": "1.18.3",
    "nyc": "^13.1.0",
    "pre-commit": "1.2.2",
    "redis-mock": "0.20.0",
    "sinon": "5.0.5"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-object-rest-spread"
    ],
    "sourceMaps": true,
    "retainLines": true
  },
  "pre-commit": [
    "lint"
  ],
  "nyc": {
    "exclude": [
      "**/*.spec.js",
      "tests/env.js"
    ]
  }
}
